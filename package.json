  "scripts": {
    "dev": "tsx server/index.ts",
    "dev:frontend": "vite",
    "dev:functions": "netlify dev --port 9999",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:functions\"",
    "dev:local": "cross-env NODE_ENV=development concurrently \"npm run dev:frontend\" \"netlify functions:serve --port 9999\"",
    "dev:watch": "nodemon --watch server --watch client --ext ts,tsx,js,jsx,css,json --ignore 'node_modules/*' --exec 'npm run dev'",
    "dev:live": "concurrently \"npm run dev:watch\" \"npm run dev:frontend\"",
    "start": "npm run dev:all",
    "fix-imports": "echo 'Setting up environment'",
    "prebuild": "node scripts/generate-env-config.js",
    "build": "vite build",
    "postbuild": "node scripts/postbuild.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
